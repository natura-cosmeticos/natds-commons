<?xml version="1.0" encoding="utf-8"?>
<!-- 
{{> fileHeader }}
-->
<resources>
  <style name="Theme.{{brandName}}.{{mode}}" parent="Theme.MaterialComponents.{{materialMode}}">
    {{#each colors}}
    <item name="{{name}}">@color/{{value}}</item>
    {{/each}}
    {{#each dimensions}}
    <item name="{{name}}">@dimen/{{value}}</item>
    {{/each}}
    {{#each drawables}}
    <item name="{{name}}">@drawable/{{value}}</item>
    {{/each}}
    {{#each fonts}}
    <item name="{{name}}">@font/{{value}}</item>
    {{/each}}
    {{#each properties}}
    <item name="{{name}}">{{value}}</item>
    {{/each}}
  </style>
</resources>
