{
  "name": "@naturacosmeticos/natds-icons",
  "displayName": "NatDS Icons",
  "version": "0.18.24",
  "private": false,
  "description": "A collection of icons for Natura Design System",
  "keywords": [
    "design-system",
    "icons",
    "natds",
    "natura"
  ],
  "homepage": "https://github.com/natura-cosmeticos/natds-js/tree/master/packages/icons/README.md",
  "bugs": {
    "url": "https://github.com/natura-cosmeticos/natds-js/issues"
  },
  "license": "ISC",
  "author": "Natura Cosméticos <designsystem@natura.net>",
  "contributors": [
    "Ariel Flor <aflor@thoughtworks.com>",
    "Daniel Castro <daniel.castro@vizir.com.br>",
    "Jimmy Andrade <jimmy.andrade@thoughtworks.com>",
    "Matheus Robert Lichtnow <matheus@vizir.com.br>",
    "Vagner Saraiva <vagner@vizir.com.br>",
    "Vicente Lyrio <vicente.lyrio@vizir.com.br>",
    "Vinícius Ponciano <vinicius.ponciano@thoughtworks.com>"
  ],
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "browser": "dist/umd/index.js",
  "jsdelivr": "dist/umd/index.js",
  "source": "src/index.ts",
  "types": "dist/index.d.ts",
  "typings": "dist/index.d.ts",
  "unpkg": "dist/umd/index.js",
  "repository": {
    "directory": "packages/icons",
    "type": "git",
    "url": "https://github.com/natura-cosmeticos/natds.git"
  },
  "scripts": {
    "build": "yarn clean && yarn minify-svg-files && yarn create-font-files && yarn build-typescript && yarn copy-fonts && yarn copy-css && yarn generate-typings && yarn generate-json-type && yarn copy-typings && yarn generate-umd",
    "build-typescript": "tsc -p ./tsconfig.json",
    "clean": "rm -rf dist && rm -rf src/assets/cleaned/* && rm -rf src/fonts/*",
    "copy-css": "cp -r ./src/*.css* ./dist",
    "copy-fonts": "cp -r ./src/fonts ./dist",
    "copy-typings": "cp -r ./src/*.d.ts ./dist",
    "create-font-files": "node ./scripts/svgToFont.js",
    "generate-json-type": "node ./scripts/generateJsonType.js",
    "generate-typings": "tcm -o src src",
    "generate-umd": "rollup -c",
    "minify-svg-files": "node ./scripts/svgCleaner.js",
    "start": "http-server ./src",
    "test": "jest --collectCoverage --colors",
    "watch": "yarn start",
    "lint": "eslint --ext .js ./",
    "lint:fix": "yarn lint --fix"
  },
  "config": {},
  "devDependencies": {
    "@rollup/plugin-json": "4.1.0",
    "@rollup/plugin-typescript": "6.0.0",
    "@rollup/plugin-url": "5.0.1",
    "babel-jest": "26.5.2",
    "glob": "7.1.6",
    "jest": "26.4.2",
    "json-to-ts": "1.7.0",
    "rollup": "2.26.7",
    "svgo": "1.3.2",
    "tslib": "2.0.1",
    "typed-css-modules": "0.6.4",
    "typescript": "4.0.2",
    "webfont": "9.0.0"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "puppeteer": "^5.3.1"
  }
}
