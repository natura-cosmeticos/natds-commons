<html lang="en">

<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Color tokens</title>
  <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.5.0/css/bootstrap.min.css"
    integrity="sha384-9aIt2nRpC12Uk9gS9baDl411NQApFmC26EwAOH8WgZl5MYYxFfc+NcPb1dKGj7Sk" crossorigin="anonymous">
  <link href="https://fonts.googleapis.com/css2?family=Roboto:wght@400;500&display=swap" rel="stylesheet">
  <link href="assets/{{brandName}}_fonts.css" rel="stylesheet">



</head>

<body>
  <style>
    * {
      font-family: 'Roboto', sans-serif;
    }

    .color-sample {
      border: 1px solid #eaeaea;
      height: 50px;
    }

    .opacity-sample {
      background-color: #000000;
    }

    .size-sample {
      background: #ffe5e5;
    }

    .spacing-sample {
      height: 120px;
      background-color: #f3f2f2;
    }

    .spacing-element {
      background-color: #ffe5e5;
    }

    .spacing-inner-element {
      background-color: #aba6a6;
      height: 20px;
    }

    .borderRadius-sample {
      background: #ffe5e5;
      border: 1px solid #e83e8c;
      height: 70px;
      width: 100%;
    }

    .typography-sample {
      background-color: #f3f2f2;
      padding: 10px;
    }
    code {
      white-space: nowrap;
    }

    {{#each typography}}
      .{{name}} {
        font-family: {{fontFamily}};
        font-weight: {{fontWeight}};
      }  
    {{/each}}

    .button {
      border-radius: {{components.button.borderRadius.value}};
      border: 1px solid black;
    }

    .button-wrapper{
        background-color: {{tokens.color.surface.value}};
    }

    .contained{
      background-color: {{components.button.contained.color.enable.background.value}};
      color: {{components.button.contained.color.enable.label.value}};
      border-color: {{components.button.contained.color.enable.border.value}};
    }
    .contained:hover{
      background-color: {{components.button.contained.color.hover.background.value}};
      color: {{components.button.contained.color.hover.label.value}};
      border-color: {{components.button.contained.color.hover.border.value}};
    }
    .contained:focus{
      background-color: {{components.button.contained.color.focus.background.value}};
      color: {{components.button.contained.color.focus.label.value}};
      border-color: {{components.button.contained.color.focus.border.value}};
    }
    .contained.disable{
      background-color: {{components.button.contained.color.disable.background.value}};
      color: {{components.button.contained.color.disable.label.value}};
      border-color: {{components.button.contained.color.disable.border.value}};
    }
    .outlined{
      background-color: {{components.button.outlined.color.enable.background.value}};
      color: {{components.button.outlined.color.enable.label.value}};
      border-color: {{components.button.outlined.color.enable.border.value}};
    }
    .outlined:hover{
      background-color: {{components.button.outlined.color.hover.background.value}};
      color: {{components.button.outlined.color.hover.label.value}};
      border-color: {{components.button.outlined.color.hover.border.value}};
    }
    .outlined:focus{
      background-color: {{components.button.outlined.color.focus.background.value}};
      color: {{components.button.outlined.color.focus.label.value}};
      border-color: {{components.button.outlined.color.focus.border.value}};
    }
    .outlined.disable{
      background-color: {{components.button.outlined.color.disable.background.value}};
      color: {{components.button.outlined.color.disable.label.value}};
      border-color: {{components.button.outlined.color.disable.border.value}};
    }
    
    .text{
      background-color: {{components.button.text.color.enable.background.value}};
      color: {{components.button.text.color.enable.label.value}};
      border-color: {{components.button.text.color.enable.border.value}};
    }
    .text:hover{
      background-color: {{components.button.text.color.hover.background.value}};
      color: {{components.button.text.color.hover.label.value}};
      border-color: {{components.button.text.color.hover.border.value}};
    }
    .text:focus{
      background-color: {{components.button.text.color.focus.background.value}};
      color: {{components.button.text.color.focus.label.value}};
      border-color: {{components.button.text.color.focus.border.value}};
    }
    .text.disable{
      background-color: {{components.button.text.color.disable.background.value}};
      color: {{components.button.text.color.disable.label.value}};
      border-color: {{components.button.text.color.disable.border.value}};
    }
    
    .ds-badge {
      font-size: {{components.badge.label.fontSize.value}};
      letter-spacing: {{components.badge.label.letterSpacing.value}};
      line-height: {{components.badge.label.lineHeight.value}};
      font-family: {{components.badge.label.primary.fontFamily.value}};
      font-weight: {{components.badge.label.primary.fontWeight.value}};
    }
    .ds-badge.ds-standard {
      height: {{components.badge.standard.height.value}};
      border-radius: {{components.badge.standard.borderRadius.value}};
    }
    
    .ds-badge.ds-dot {
      height: {{components.badge.dot.height.value}};
      width: {{components.badge.dot.height.value}};
      border-radius: {{components.badge.dot.borderRadius.value}};
    }
    
    .ds-badge.ds-primary {
      background-color: {{components.badge.color.primary.background.value}};
      color: {{components.badge.color.primary.label.value}};
    }
    .ds-badge.ds-secondary {
      background-color: {{components.badge.color.secondary.background.value}};
      color: {{components.badge.color.secondary.label.value}};
    }
    .ds-badge.ds-success {
      background-color: {{components.badge.color.success.background.value}};
      color: {{components.badge.color.success.label.value}};
    }
    .ds-badge.ds-alert {
      background-color: {{components.badge.color.alert.background.value}};
      color: {{components.badge.color.alert.label.value}};
    }
    .ds-shortcut {
      background-color: {{tokens.color.primary.value}};
      color: {{tokens.color.highEmphasis.value}};
      font-family: {{components.shortcut.label.primary.fontFamily.value}};
      font-weight: {{components.shortcut.label.primary.fontWeight.value}};
      height: {{tokens.size.mediumX.value}};
      letter-spacing: {{components.shortcut.label.letterSpacing.value}};
      line-height: {{components.shortcut.label.lineHeight.value}};
      width: {{tokens.size.mediumX.value}};
      border-radius: {{components.shortcut.borderRadius.value}};
      display: flex;
      align-items: center;
      justify-content: center;
    }
    
  </style>
  {{> navigation }}
  <div class="container-fluid mt-5">
    <div class="row">
      <div class="col-12">
        <h1>Colors</h1>
        <hr>
      </div>
      {{#each tokens.color}}
      <div class="color col-12 col-sm-6 col-md-4 col-lg-3">
        <p class="color-name mt-5 mb-1">{{name}}: <code>{{value}}</code></p>
        <div class="color-sample" style="background: {{value}}"></div>
      </div>
      {{/each}}
      <div class="col-12 mt-5">
        <h1>Opacities</h1>
        <hr>
      </div>
      {{#each tokens.opacity}}
      <div class="color col-12 col-sm-6 col-md-4 col-lg-3">
        <p class="color-name mt-5 mb-1">{{name}}: <code>{{value}}</code></p>
        <div class="color-sample opacity-sample" style="opacity: {{value}}"></div>
      </div>
      {{/each}}
      <div class="col-12 mt-5">
        <h1>Elevations</h1>
        <hr>
      </div>
      {{#each tokens.elevation}}
      <div class="color col-12 col-sm-6 col-md-4 col-lg-3">
        <p class="color-name mt-5 mb-1">{{name}}: <code>{{value}}</code></p>
        <div class="color-sample" style="box-shadow: {{value}}"></div>
      </div>
      {{/each}}
      <div class="col-12 mt-5">
        <h1>Sizes</h1>
        <hr>
      </div>
      {{#each tokens.size}}
      <div class="color col-12 col-sm-6 col-md-4 col-lg-3">
        <p class="color-name mt-5 mb-1">{{name}}: <code>{{value}}</code></p>
        <div class="size-sample" style="height: {{value}}px"></div>
      </div>
      {{/each}}
      <div class="col-12 mt-5">
        <h1>Spacing</h1>
        <hr>
      </div>
      {{#each tokens.spacing}}
      <div class="color col-12 col-sm-6 col-md-4 col-lg-3">
        <p class="color-name mt-5 mb-1">{{name}}: <code>{{value}}</code></p>
        <div class="spacing-sample">
          <div class="spacing-element" style="height: {{value}}px"></div>
          <div class="spacing-inner-element"></div>
        </div>
      </div>
      {{/each}}
      <div class="col-12 mt-5">
        <h1>Border Radius</h1>
        <hr>
      </div>
      {{#each tokens.borderRadius}}
      <div class="color col-12 col-sm-6 col-md-4 col-lg-3">
        <p class="color-name mt-5 mb-1">{{name}}: <code>{{value}}</code></p>
        <div class="borderRadius-sample" style="border-radius: {{value}}px"></div>
      </div>
      {{/each}}
      <div class="col-12 mt-5">
        <h1>Logo</h1>
        <hr>
      </div>
      {{#each brand}}
      <div class="color col-12 col-sm-6 col-md-4 col-lg-3">
        <p class="color-name mt-5 mb-1">{{name}}</p>
        <div class="logo-sample {{attributes.item}}">{{value}}</div>
      </div>
      {{/each}}
      <div class="col-12 mt-5">
        <h1>Typography</h1>
        <hr>
        <h3>Font Family</h1>
      </div>
      {{#each typography}}
      <div class="color col-12 col-sm-6 col-md-4">
        <p class="color-name mt-5 mb-1">{{name}}: <code>{{fontFamily}}</code></p>
        <p class="typography-sample {{name}}">
          Lorem ipsum dolor sit amet, consectetur adipiscing elit.
        </p>
      </div>
      {{/each}}
      {{#each tokens.typography.lineHeight}}
      <div class="color col-12 col-sm-6 col-md-4 col-lg-3">
        <p class="color-name mt-5 mb-1">{{name}}: <code>{{value}}</code></p>
        <p class="typography-sample" style="line-height: {{value}}em">
          Lorem ipsum dolor sit amet, consectetur adipiscing elit. Aliquam non mollis dui, id egestas risus. Quisque
          blandit felis in risus tempor cursus.
        </p>
      </div>
      {{/each}}
      <div class="col-12 mt-5">
        <h1>Elements</h1>
        <hr>
      </div>
      {{#each typographyElements}}
      <h2 class="color-name col-12 mt-3 mb-1">{{@key}}</h2>
      <div class="color col-12">
        {{#each @this}}
        <p class="color-name mt-1 mb-1">{{name}}: <code>{{value}}</code></p>
        {{/each}}
        <p class="typography-sample mt-2"
          style="font-size: {{fontSize.value}}px; font-weight: {{fontWeight.value}}; letter-spacing: {{letterSpacing.value}}">
          Lorem ipsum dolor sit amet, consectetur adipiscing elit.
        </p>
      </div>
      {{/each}}
      <div class="col-12 mt-5">
        <h1>Components</h1>
        <hr>
      </div>
      <h2 class="color-name col-12 mt-3 mb-1">Button</h2>
      <div class="button-wrapper col-12">
        <button class="button contained">contained</button>
        <button class="button contained disable">contained</button>
        <button class="button outlined">outlined</button>
        <button class="button outlined disable">outlined</button>
        <button class="button text">text</button>
        <button class="button text disable">text</button>
      </div>
    </div>
      <h2 class="color-name col-12 mt-3 mb-1">Badge</h2>
      <div class="button-wrapper col-12">
        <span class="ds-badge ds-standard ds-primary">+99</span>
        <span class="ds-badge ds-dot ds-primary"></span>
        <span class="ds-badge ds-standard ds-secondary">+99</span>
        <span class="ds-badge ds-dot ds-secondary"></span>
        <span class="ds-badge ds-standard ds-success">+99</span>
        <span class="ds-badge ds-dot ds-success"></span>
        <span class="ds-badge ds-standard ds-alert">+99</span>
        <span class="ds-badge ds-dot ds-alert"></span>
      </div>
    </div>
      <h2 class="color-name col-12 mt-3 mb-1">Shortcut</h2>
      <div class="button-wrapper col-12">
        <div class="ds-shortcut">DS</div>
      </div>
    <hr>
  </div>

  <script src="https://code.jquery.com/jquery-3.5.1.slim.min.js"
    integrity="sha384-DfXdz2htPH0lsSSs5nCTpuj/zy4C+OGpamoFVy38MVBnE+IbbVYUew+OrCXaRkfj"
    crossorigin="anonymous"></script>
  <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.5.0/js/bootstrap.min.js"
    integrity="sha384-OgVRvuATP1z7JjHLkuOU7Xw704+h835Lr+6QL9UvYjZE3Ipu6Tp75j7Bh/kR0JKI"
    crossorigin="anonymous"></script>
</body>

</html>
