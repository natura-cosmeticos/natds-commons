{{> fileHeader }}

protocol Theme {
  var tokens: Tokens { get }
  var components: Components { get }
}

protocol Tokens {
{{#each tokens}}
  var {{name}}: {{attributes.customOptions.type}} { get }
{{/each}}
}

protocol Components {
{{#each components}}
  var {{name}}: {{attributes.customOptions.type}} { get }
{{/each}}
}
