{
  "radioButton": {
    "label": {
      "fontSize": {
        "value": "{body2.fontSize.value}"
      },
      "letterSpacing": {
        "value": "{body2.letterSpacing.value}"
      },
      "lineHeight": {
        "value": "{typography.lineHeight.medium.value}"
      },
      "primary": {
        "fontFamily": {
          "value": "{typography.fontFamily.primary.value}"
        },
        "fontWeight": {
          "value": "{typography.fontWeight.regular.value}"
        }
      },
      "fallback": {
        "fontFamily": {
          "value": "{platform.typography.fallback.fontFamily.value}"
        },
        "fontWeight": {
          "value": "{platform.typography.fallback.fontWeight.value}"
        }
      }
    },
    "borderRadius": {
      "value": "{private.borderRadius.circle.standard.value}"
    },
    "color": {
      "select": {
        "border": {
          "value": "{color.primary.value}"
        },
        "content": {
          "value": "{color.primary.value}"
        }
      },
      "unselect": {
        "border": {
          "value": "{color.highlight.value}"
        },
        "content": {
          "value": "{color.highlight.value}"
        }
      },
      "disable": {
        "border": {
          "value": "{color.lowEmphasis.value}"
        },
        "content": {
          "value": "{color.lowEmphasis.value}"
        }
      }
    }
  }
}
